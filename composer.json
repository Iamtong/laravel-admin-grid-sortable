{
    "name": "iamtong/laravel-admin-grid-sortable",
    "description": "Sort the grid data by drag and drop rows",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "grid", "sortable"],
    "homepage": "https://github.com/Iamtong/laravel-admin-grid-sortable",
    "license": "MIT",
    "authors": [
        {
            "name": "Iamtong",
            "email": "jiantong912@163.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": ">=1.7.2",
        "spatie/eloquent-sortable": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "iamtong\\LaravelAdmin\\GridSortable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "iamtong\\LaravelAdmin\\GridSortable\\GridSortableServiceProvider"
            ]
        }
    }
}
